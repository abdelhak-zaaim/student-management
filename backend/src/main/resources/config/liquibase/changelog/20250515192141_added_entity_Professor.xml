    <!--
        Added the entity Professor.
    -->
    <changeSet id="20250515192141-1" author="zaaims">
        <createTable tableName="professor">
            <column name="id" type="bigint">
                <constraints primaryKey="true" nullable="false"/>
            </column>
        </createTable>
    </changeSet>



